{
  "name": "GRIP",
  "description": "Resilience analysis",
  "docker": "lfenergy/arras:develop",
  "git": "https://github.com/openfido/resilience.git",
  "branch": "main",
  "script": "openfido.sh",
  "manual": {
    "config": "csv"
  },
  "config": {
     "USECASE": {
      "prompt": "USECASE",
      "description": "Type of analysis to conduct",
      "choices": "BULK, INCLUDE_NETWORK, INCLUDE_VEGETATION",
      "default": "BULK",
      "input_type": "enum"
    },
    "INPUT_POLE_FILE": {
      "prompt": "Include pole data (csv/xlxs)",
      "description": "Pole data, csv/xlxs",
      "default": "",
      "input_type": "upload"
    },
    "INPUT_EQUIPMENT_FILE": {
      "prompt": "Include equipment data (csv/xlxs)",
      "description": "Equipment data, csv/xlxs (optional)",
      "default": "",
      "input_type": "upload"
    },
    "Pole Analysis options": { "input_type": "title" },
    "STARTTIME": {
      "description": "Start of the simulation",
      "prompt": "Start Time",
      "default": "2020-01-01T00:00:00",
      "input_type": "str"
    },
    "STOPTIME": {
      "description": "End of the simulation",
      "prompt": "(optional) Wind Speed (default 8.8233 m/s)",
      "default": "2020-01-02T00:00:00",
      "input_type": "str"
    },
    "TIMEZONE": {
      "description": "Timezone of the simulation",
      "prompt": "Timezone",
      "default": "PST+8PDT",
      "input_type": "str"
    },
    "WIND_SPEED": {
      "description": "Simulation Wind Speed for all the poles",
      "prompt": "Wind Speed File (csv)",
      "default": "",
      "input_type": "upload"
    }
  }
}
